{
  "name": "ecphp/ecas",
  "description": "CAS library to communicate with ECAS / EU Login",
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">= 8.1",
    "ecphp/cas-lib": "^2"
  },
  "require-dev": {
    "ext-pcov": "*",
    "ecphp/php-conventions": "^1",
    "friends-of-phpspec/phpspec-code-coverage": "^6",
    "infection/infection": "^0.26",
    "infection/phpspec-adapter": "^0.2",
    "monolog/monolog": "^1.0 || ^3.0",
    "nyholm/psr7": "^1.2.1",
    "nyholm/psr7-server": "^1.0.0",
    "phpspec/phpspec": "^7",
    "phpstan/phpstan-strict-rules": "^1.0",
    "symfony/cache": "^6.1",
    "symfony/http-client": "^6.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "EcPhp\\Ecas\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "changelog-unreleased": "auto-changelog -c .auto-changelog -u",
    "changelog-version": "auto-changelog -c .auto-changelog -v",
    "grumphp": "./vendor/bin/grumphp run",
    "phpspec": "./vendor/bin/phpspec run"
  }
}
